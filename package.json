{
  "name": "toolboilerplate-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "emulators": "firebase emulators:start",
    "emulators:data": "firebase emulators:start --import=./emulators-data",
    "emulators:gen": "firebase emulators:start --import=./new-emulators-data --export-on-exit",
    "development": "next dev",
    "dev": "concurrently --kill-others \"yarn emulators\" \"yarn development\"",
    "build": "rm -rf out && next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest --runInBand --watch",
    "test:coverage": "jest --coverage --silent",
    "cypress": "cypress open"
  },
  "dependencies": {
    "firebase": "^9.12.1",
    "formik": "^2.2.9",
    "next": "12.3.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rsuite": "^5.20.0",
    "styled-components": "^5.3.6",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.19.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.2.0",
    "@types/node": "18.11.3",
    "@types/react": "18.0.21",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "autoprefixer": "^10.4.12",
    "babel-plugin-styled-components": "^2.0.7",
    "concurrently": "^7.5.0",
    "cypress": "^10.10.0",
    "cypress-firebase": "^2.2.2",
    "eslint": "^8.0.1",
    "eslint-config-next": "12.3.1",
    "eslint-config-standard-with-typescript": "^23.0.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jest": "^27.1.3",
    "eslint-plugin-n": "^15.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-testing-library": "^5.9.1",
    "firebase-admin": "^11.2.0",
    "jest": "^29.2.1",
    "jest-environment-jsdom": "^29.2.1",
    "postcss": "^8.4.18",
    "tailwindcss": "^3.2.0",
    "ts-jest": "^29.0.3",
    "typescript": "*"
